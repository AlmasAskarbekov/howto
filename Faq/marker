convert -background none -gravity South shadow*.* +append shadow.png
convert -background none -fill white -font MyriadPro-Bold -pointsize 42 label:'Стенка ЛИМА' -trim \( +clone -background gray -shadow 100x4+0+0 \) +swap -background none -layers merge +repage shadow0.png
